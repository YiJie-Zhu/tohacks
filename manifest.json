{
  "name": "Coronavirus Extension",
  "version": "1.0",
  "description": "Block/Highlight Coronavirus",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.google.com/*"],
      "js": ["script.js"]
    },
    {
      "matches": ["https://www.reddit.com/*"],
      "js": ["reddit.js"]
    }
  ]
}
